<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"  xmlns:f="sap.ui.layout.form">
	<Dialog id="_IDGenDialog1" title="{i18n>nuevoAscenso}">
		<content>
			<f:Form id="_IDGenForm1">
					<f:layout>
						<f:ResponsiveGridLayout id="_IDGenResponsiveGridLayout1"
							labelSpanXL="3"
							labelSpanL="3"
							labelSpanM="3"
							labelSpanS="12"
							adjustLabelSpan="false"
							emptySpanXL="4"
							emptySpanL="4"
							emptySpanM="4"
							emptySpanS="0"
							columnsXL="1"
							columnsL="1"
							columnsM="1"
							singleContainerFullSize="false" />
					</f:layout>
					<f:formContainers>
						<f:FormContainer id="_IDGenFormContainer1">
							<f:formElements>
								<f:FormElement id="_IDGenFormElement1" label="{i18n>salario}">
									<f:fields>
										<Input value="{newRise>/Ammount}" id="name"/>
									</f:fields>
								</f:FormElement>
								<f:FormElement id="_IDGenFormElement2" label="{i18n>fecha}">
									<f:fields>
										<DatePicker id="_IDGenDatePicker1" dateValue="{newRise>/CreationDate}"></DatePicker>
									</f:fields>
								</f:FormElement>
								<f:FormElement id="_IDGenFormElement3" label="{i18n>comentario}">
									<f:fields>
										<TextArea id="_IDGenTextArea1" value="{newRise>/Comments}"></TextArea>
									</f:fields>
								</f:FormElement>
							</f:formElements>
						</f:FormContainer>
					</f:formContainers>
			</f:Form>
		</content>
		<buttons>
			<Button id="_IDGenButton1" text="{i18n>aceptar}" press="addRise"></Button>
			<Button id="_IDGenButton2" text="{i18n>cancelar}" press="onCloseRiseDialog"></Button>
		</buttons>
	</Dialog>
</core:FragmentDefinition>